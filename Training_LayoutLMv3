      "metadata": {
        "id": "33dJrfnYTbFO"
      },
      "execution_count": 8,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "import matplotlib.pyplot as plt\n",
        "\n",
        "torch.cuda.empty_cache()\n",
        "\n",
        "optimizer = AdamW(model.parameters(), lr=5e-5)\n",
        "device = torch.device('cuda')\n",
        "global_step = 0\n",
        "num_train_epochs = 4\n",
        "t_total = len(train_dataloader) * num_train_epochs\n",
        "\n",
        "# Store accuracies over steps\n",
        "train_accuracies = []\n",
        "eval_accuracies = []\n",
        "\n",
        "model.to(device)\n",
        "scheduler = get_linear_schedule_with_warmup(optimizer, num_warmup_steps=0, num_training_steps=t_total)\n",
        "\n",
        "log_interval = 20  # Log accuracy every 20 steps\n",
        "\n",
        "for epoch in range(num_train_epochs):\n",
        "    total_loss = 0\n",
        "    total_accuracy = 0\n",
        "    num_steps = 0\n",
        "\n",
        "    print(f\"Epoch {epoch + 1}/{num_train_epochs}\")\n",
        "\n",
        "    # Training loop\n",
        "    for step, batch in enumerate(tqdm(train_dataloader, desc=\"Training\")):\n",
        "        input_ids = batch['input_ids'].to(device)\n",
        "        bbox = batch['bbox'].to(device)\n",
        "        attention_mask = batch['attention_mask'].to(device)\n",
        "        token_type_ids = batch['token_type_ids'].to(device)\n",
        "        labels = batch['labels'].to(device)\n",
        "\n",
        "        # Forward pass\n",
        "        outputs = model(input_ids=input_ids, bbox=bbox, attention_mask=attention_mask, token_type_ids=token_type_ids,\n",
        "                        labels=labels)\n",
        "        loss = outputs.loss\n",
        "        logits = outputs.logits\n",
        "\n",
        "        predictions = torch.argmax(logits, dim=2)\n",
        "\n",
        "        active_loss = labels != -100\n",
        "        active_logits = logits[active_loss]\n",
        "        active_labels = labels[active_loss]\n",
        "\n",
        "        # Calculate accuracy\n",
        "        total_accuracy += torch.sum(torch.argmax(active_logits, dim=1) == active_labels).item() / torch.sum(active_loss).item()\n",
        "        total_loss += loss.item()\n",
        "        num_steps += 1\n",
        "\n",
        "        # Backward pass and optimization\n",
        "        loss.backward()\n",
        "        optimizer.step()\n",
        "        scheduler.step()\n",
        "        optimizer.zero_grad()\n",
        "\n",
        "        global_step += 1\n",
        "\n",
        "        # Log and save accuracy every 'log_interval' steps\n",
        "        if step % log_interval == 0 and step != 0:\n",
        "            train_accuracy = total_accuracy / num_steps\n",
        "            train_accuracies.append(train_accuracy)\n",
        "\n",
        "            print(f\"Step {step}: Train Accuracy={train_accuracy}\")\n",
        "\n",
        "    # Evaluation loop (after each epoch)\n",
        "    model.eval()\n",
        "    eval_accuracy = 0\n",
        "    eval_steps = 0\n",
        "    with torch.no_grad():\n",
        "        for batch in tqdm(eval_dataloader, desc=\"Evaluating\"):\n",
        "            input_ids = batch['input_ids'].to(device)\n",
        "            bbox = batch['bbox'].to(device)\n",
        "            attention_mask = batch['attention_mask'].to(device)\n",
        "            token_type_ids = batch['token_type_ids'].to(device)\n",
        "            labels = batch['labels'].to(device)\n",
        "\n",
        "            outputs = model(input_ids=input_ids, bbox=bbox, attention_mask=attention_mask, token_type_ids=token_type_ids)\n",
        "            logits = outputs.logits\n",
        "\n",
        "            active_loss = labels != -100\n",
        "            active_logits = logits[active_loss]\n",
        "            active_labels = labels[active_loss]\n",
        "\n",
        "            eval_accuracy += torch.sum(torch.argmax(active_logits, dim=1) == active_labels).item() / torch.sum(active_loss).item()\n",
        "            eval_steps += 1\n",
        "\n",
        "    eval_accuracy /= eval_steps\n",
        "    eval_accuracies.append(eval_accuracy)\n",
        "\n",
        "    print(f\"Epoch {epoch + 1} completed: Eval Accuracy={eval_accuracy}\")\n",
        "\n",
        "    # Retourner en mode entraînement\n",
        "    model.train()\n",
        "\n",
        "# Plot the accuracies\n",
        "steps = range(len(train_accuracies))  # X-axis based on logged intervals\n",
        "plt.plot(steps, train_accuracies, label='Train Accuracy')\n",
        "plt.plot(range(len(eval_accuracies)), eval_accuracies, label='Eval Accuracy', marker='o')  # One point per epoch for eval\n",
        "plt.xlabel('Steps')\n",
        "plt.ylabel('Accuracy')\n",
        "plt.legend(loc='upper left')\n",
        "plt.title('Train Accuracy per Step and Eval Accuracy per Epoch')\n",
        "plt.show()\n"
      ],
